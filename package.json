{
  "name": "doodlr",
  "version": "0.0.0-semantic-release",
  "description": "doodlr",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./ --ignore-pattern node_modules/ --ignore-pattern lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "npm run lint && npm run compile && nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dustinspecker/doodlr.git"
  },
  "keywords": [],
  "author": {
    "name": "Dustin Specker",
    "email": "DustinSpecker@DustinSpecker.com",
    "url": "github.com/dustinspecker"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.4.0",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.4.3",
    "coveralls": "^2.11.6",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.4.0",
    "eslint-config-dustinspecker": "^0.4.1",
    "eslint-plugin-no-use-extend-native": "^0.3.1",
    "json-loader": "^0.5.4",
    "nyc": "^6.1.1",
    "semantic-release": "^4.3.5",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "ava": {
    "files": [
      "tests/test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  }
}
